{
  "name": "typescript-webpack-jest-boilerplate",
  "version": "0.0.1",
  "description": "typescript webpack jest boilerplate",
  "main": "src/index.ts",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "awesome-typescript-loader": "^3.4.1",
    "babel-jest": "^22.0.4",
    "clean-webpack-plugin": "^0.1.17",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.3",
    "jest-extended": "^0.5.0",
    "jsdom": "^11.5.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --open --config=./builds/webpack.dev.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/tests/*.spec.(ts|tsx|js)"
    ],
    "setupTestFrameworkScriptFile": "jest-extended",
    "globals": {
      "document": true
    }
  },
  "keywords": [
    "typescript",
    "webpack",
    "jest"
  ],
  "author": "maple-leaf",
  "license": "MIT"
}
